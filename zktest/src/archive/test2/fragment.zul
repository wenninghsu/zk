<nodom viewModel="@id('vm') @init('org.zkoss.zktest.test2.FragmentVM')">
	<fragment id="fg" customer="@bind(vm.customer)"><![CDATA[
		<span>{{customer.name}}</span>
		<span>{{customer.age}}</span>
		<span>{{customer.nation}}</span>
		<input v-model="customer.age" v-on:change="onChangeMethod"/>
	]]></fragment>
	<button onClick="fg.invalidate()" label="fragment invalidate"/>
